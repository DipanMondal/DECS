{
	"version": 1.0,
	"builds":[
		{
			"src": "DECS/wsgi.py",
			"use": "@vercel/python",
			"config": {"maxLambdaSize":"15mb","runtime":"python3.9"}
		},
		{
        		"src": "build_files.sh",
        		"use": "@vercel/static"
        }
	],
	"routes":[
		{
			"src": "/staticfiles/(.*)",
        		"dest": "/staticfiles/$1"
		},
		{
			"src": "/(.*)",
			"dest": "DECS/wsgi.py"
		}
	]
}